   "source": [
    "A=[-2,-1,-3,-4,-1]\n",
    "def solution(A):\n",
    "    max_ending = max_slice = 0\n",
    "    if max(A) < 0:\n",
    "        return max(A)\n",
    "    for i in A:\n",
    "        max_ending = max(0, max_ending+i)\n",
    "        max_slice = max(max_ending, max_slice)\n",
    "    return max_slice"
   ]
