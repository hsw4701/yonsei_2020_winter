   "source": [
    "def solution(A):\n",
    "    A.sort()\n",
    "    n=len(A)\n",
    "    x1 = A[n-1]*A[n-2]*A[n-3]\n",
    "    x2 = A[0]*A[1]*A[n-1]\n",
    "    return max(x1,x2)"
   ]
  }
