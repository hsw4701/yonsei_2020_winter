    "def solution(A):\n",
    "    A1=set(A)\n",
    "    A2=set(range(1,max(A1)+1))\n",
    "    sol=[]\n",
    "    if len(A2-A1) == 0 and len(A2) == 0 :\n",
    "        sol.append(1)\n",
    "    elif len(A2-A1) == 0 and len(A2) > 0 :\n",
    "        sol.append(max(A2)+1)\n",
    "    else:\n",
    "        sol.append(min(A2-A1))\n",
    "    return sol[0]"
   ]
