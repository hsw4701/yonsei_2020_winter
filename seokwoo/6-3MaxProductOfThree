def solution(A):
    A.sort()
    A1=[]
    A2=[]
    for a in A:
        if a<0:
            A1.append(a)
    if len(A1) <= 1:
        return (A[-1]*A[-2]*A[-3])
    elif len(A1)>=2:
        if A1[0]*A1[1]*A[-1] > A[-1]*A[-2]*A[-3]:
            return (A1[0]*A1[1]*A[-1])
        else :
            return (A[-1]*A[-2]*A[-3])
O(N * log(N))
